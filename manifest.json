{
  "manifest_version": 2,

  "name": "Search GitHub Starred",
  "version": "1.0",
  "description": "Do something special.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
    /* "default_popup": "index.html" */
  },
  "permissions": [
    "activeTab",
    "storage",
    "identity",
    "https://github.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "key": ""
}
